{
    "name": "supertalk-vscode",
    "displayName": "Supertalk",
    "description": "Supertalk language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "supertalk",
            "aliases": ["Supertalk", "supertalk"],
            "extensions": [".sts"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "supertalk",
            "scopeName": "source.supertalk",
            "path": "./syntaxes/supertalk.tmLanguage.json"
        }]
    }
}